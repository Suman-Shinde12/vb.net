using System;
namespace consoleApplication2 {
    class program
    {
        static void Main(string[] args)
        {
            string str1;
            Console.Write("Enter string 1:");
            str1 = Console.ReadLine();
            string[] words = str1.Split(',');
            for (int i = 0; i < words.Length; i++)
            {
                Console.Write("\"" + words[i] + "\"");
                Console.ReadKey();
            }
        }

    }

}
